---
title: blogPost
---
<!doctype html>
<link rel="stylesheet" href="/bundle.css">
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title }}</title>
  </head>
  <body>
    <h1>{{title}}</h1>
    {% if thumbnail %}
      <img alt={{thumbnail}} src={{thumbnail}}>
    {% endif %}
    {{ content | safe }}
  </body>
</html>